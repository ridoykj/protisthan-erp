  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:8088",
      "webRoot": "${workspaceFolder}/src/main/frontend"
    },
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:8088/",
      "webRoot": "${workspaceFolder}/src/main/frontend"
    },
    {
      "type": "java",
      "name": "Launch Application",
      "request": "launch",
      "mainClass": "com.itbd.protisthan"
    },
    {
      "type": "java",
      "request": "attach",
      "name": "Attach to Application started using Maven",
      "hostName": "localhost",
      "port": 5247
    },
  ]
}